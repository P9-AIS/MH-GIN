    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: main us config",
            "type": "debugpy",
            "request": "launch",
            // "program": "${file}",
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal",
            // "args": "${command:pickArgs}"
            "args": [
                "--config", "config-US.yaml"
            ]
        },
        {
            "name": "Python Debugger: data process us config",
            "type": "debugpy",
            "request": "launch",
            // "program": "${file}",
            "program": "${workspaceFolder}/src/data/american_data_process_module.py",
            "console": "integratedTerminal",
            // "args": "${command:pickArgs}"
            "args": [
                "--config", "config-US.yaml"
            ]
        }
    ]
}